    "watch": "^1.0.2",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "cross-env PORT=3000 BROWSER=chrome react-scripts start",
    "server": "node server/src/server.js",
    "dev": "node kill-ports.js && concurrently \"npm run server\" \"npm run start\"",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "kill-ports": "node kill-ports.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "A web application that integrates with Strava to visualize and analyze your cycling activities. Built with React and Node.js, this application allows you to view your activities on an interactive map, analyze heart rate data, and filter through your Strava history.",
  "main": "server/src/server.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "kill-port": "^2.0.1",
