    "@turf/turf": "^7.2.0",
    "axios": "^1.8.3",
    "chart.js": "^4.4.8",
    "cors": "^2.8.5",
    "daisyui": "^5.0.4",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.10.0",
    "mongodb": "^6.14.2",
    "mongoose": "^8.12.1",
    "react": "^19.0.0",
